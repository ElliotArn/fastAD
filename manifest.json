{
  "manifest_version": 3,
  "name": "fastAD",
  "version": "1.2",
  "description": "Speeds up YouTube ads so you don't have to watch them",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch?*"],
      "js": ["content.js"]
    }
  ],

  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ]
}
